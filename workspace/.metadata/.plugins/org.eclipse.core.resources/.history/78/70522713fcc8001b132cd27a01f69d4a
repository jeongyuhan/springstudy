package quiz03;

import java.util.List;

public class Exam {
	
	private List<Integer> scores; // 5개의 점수
	private double average; // 평균
	private char grade; // A ~ F
	
	public void info() {
		for(int i = 0, length = scores.size(); i < length; i++) {
			int no1 = scores.get(0);
			int no2 = scores.get(1);
			int no3 = scores.get(2);
			int no4 = scores.get(3);
			int no5 = scores.get(4);
			
			System.out.println("scores : [" + scores.get(i) + "]");
			System.out.println("average : " + scores.get(i)/scores.size());
			System.out.println("grade : ");
		}
	}

	public List<Integer> getScores() {
		return scores;
	}

	public void setScores(List<Integer> scores) {
		this.scores = scores;
	}

	public double getAverage() {
		return average;
	}

	public void setAverage(double average) {
		this.average = average;
	}

	public char getGrade() {
		return grade;
	}

	public void setGrade(char grade) {
		this.grade = grade;
	}
	
	
	
}
